{
  "name": "doc-gallery",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "build": "webpack --mode production",
    "start": "node dist/bundle.js",
    "deploy:staging": "git tag -l 'staging-*' | xargs -r -I {} git push origin :refs/tags/{} && TIMESTAMP=$(date +%Y%m%d%H%M%S) && TAG_NAME=staging-${TIMESTAMP} && git tag $TAG_NAME && git push origin $TAG_NAME"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "^5.0.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "docx": "^9.0.2",
    "docx-templates": "^4.13.0",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "pizzip": "^3.1.7",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "nodemon": "^3.1.7",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}